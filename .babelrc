{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-class-properties"]
  ],
  "comments": false,
  "sourceType": "unambiguous",
  "assumptions": {
    "setPublicClassFields": true,
    "privateFieldsAsProperties": true
  }
}
